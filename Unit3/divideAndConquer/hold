public static void main(String[] args) {

        int[] arr = new int[6];
        arr[0] = 1123;
        arr[1] = -21;
        arr[2] = -23;
        arr[3] = 3;
        arr[4] = 32;
        arr[5] = 455;


        Sorting sort = new Sorting();

        sort.lsdRadixSort(arr);


        for (int i = 0; i < arr.length; i++) {
            System.out.print(arr[i] + " ");
        }
     
        System.out.println("");
    }